{
    "name": "egg-vue-webpack-boilerplate",
    "version": "1.0.0",
    "description": "基于egg-vue-webpack-dev和egg-view-vue(ssr)插件的工程骨架项目",
    "scripts": {
        "build": "NODE_ENV=production node build",
        "build-dev": "NODE_ENV=development node build",
        "build-prod": "NODE_ENV=production node build",
        "kill-port": "kill -9 $(lsof -i:8888 |grep 'node'|awk '{print $2}') ",
        "start": "WORKERS=1 PORT=7003 NODE_ENV=development node index.js",
        "start-prod": "WORKERS=1 && NODE_ENV=production node index.js",
        "restart": "npm run kill-port && npm run start",
        "lint": "eslint .",
        "fix": "eslint --fix .",
        "release": "EGG_SERVER_ENV=prod PORT=7003 nohup node index.js > stdout.log 2> stderr.log &"
    },
    "dependencies": {
        "@hubcarl/egg-view-vue": "^0.0.2",
        "axios": "^0.15.3",
        "co": "^4.6.0",
        "co-wait": "^0.0.0",
        "core-js": "^2.4.0",
        "egg": "^1.1.0",
        "egg-logger": "^1.5.0",
        "egg-logview": "^1.0.0",
        "egg-mongoose": "^2.1.0",
        "egg-validate": "^1.0.0",
        "egg-view-vue-ssr": "^0.1.0",
        "element-ui": "^1.2.5",
        "extend": "~3.0.0",
        "lodash": "^4.17.4",
        "lru-cache": "^4.0.1",
        "mockjs": "^1.0.1-beta3",
        "moment": "^2.17.1",
        "serialize-javascript": "^1.3.0",
        "vue": "^2.2.4",
        "vue-resource": "^1.0.3",
        "vue-router": "^2.1.1",
        "vue-server-renderer": "^2.1.10",
        "vuex": "^2.1.1",
        "vuex-router-sync": "^4.1.2"
    },
    "devDependencies": {
        "autod-egg": "^1.0.0",
        "babel-plugin-add-module-exports": "^0.2.1",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-stage-2": "^6.24.1",
        "egg-development-proxyworker": "^1.1.0",
        "egg-webpack": "^0.1.0",
        "egg-webpack-vue": "^0.1.1",
        "eslint": "^3.18.0",
        "eslint-config-egg": "^3.2.0",
        "eslint-plugin-vue": "^2.0.1",
        "node-noop": "^1.0.0",
        "stats-webpack-plugin": "^0.6.0",
        "string-replace-webpack-plugin": "^0.1.3",
        "webpack": "^2.4.1",
        "webpack-merge": "^2.4.0"
    },
    "engines": {
        "node": ">=6.0.0"
    },
    "ci": {
        "version": "6, 7"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/hubcarl/egg-vue-webpack-boilerplate.git"
    },
    "author": "hubcarl@126.com",
    "license": "MIT",
    "homepage": "https://github.com/hubcarl/egg-vue-webpack-boilerplate"
}
